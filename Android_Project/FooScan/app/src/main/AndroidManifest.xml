<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ben.fooscan" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/dingid"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Inventory"
            android:label="@string/title_activity_inventory"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
        <activity
            android:name=".Recipe_Generator"
            android:label="@string/title_activity_recipe__generator"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
    </application>

</manifest>
